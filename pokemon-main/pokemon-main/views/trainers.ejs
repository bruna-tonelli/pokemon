<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <meta charset="UTF-8">
    <title>Treinadores</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            background-color: #f5f5f5;
            margin: 0;
            padding: 0;
            display: flex;
            flex-direction: column;
            align-items: center;
        }

        h1 {
            color: #333;
        }

        ul {
            list-style-type: none;
            padding: 0;
            width: 300px;
        }

        li {
            background-color: #fff;
            margin: 10px 0;
            padding: 15px;
            border-radius: 8px;
            box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
        }

        li a {
            text-decoration: none;
            color: #007BFF;
            font-weight: bold;
        }

        li a:hover {
            text-decoration: underline;
        }

    
    </style>
</head>
<body>
    <h1>Treinadores</h1>
    <ul>
        <% trainers.forEach(function(trainer){ %>
            <li>
                <a href="/trainers/<%= trainer.nome %>"><%= trainer.nome %></a>
            </li>
        <% }); %>
    </ul>
    <a href="/">Voltar à Pokédex</a>
</body>
</html>
